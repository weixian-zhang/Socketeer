{
  "compilerOptions": {
    "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": false,
      "sourceMap": true,
      "composite": true,
      "declaration": true,
      "declarationMap": true,
      "jsx": "react",
      "baseUrl": "./",
      "outDir": "./dist",
  }
}